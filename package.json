{
  "name": "tpl-fastify",
  "version": "8.6.0",
  "description": "Fastify API bootstrap template.",
  "author": "Henrique Alves <henrique013.a@gmail.com> (https://github.com/henrique013)",
  "license": "MIT",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "tsx watch --env-file=.env --inspect=0.0.0.0:9229 src/infra/main.ts",
    "start": "tsx src/infra/main.ts",
    "migrate": "dotenv -e .env -- npx drizzle-kit migrate",
    "migrate:gen": "tsx bin/scripts/migrate.gen.ts",
    "tag": "tsx bin/scripts/tag.ts",
    "postinstall": "tsx bin/scripts/postinstall.ts",
    "compile": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write --cache .",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@sentry/node": "^9.24.0",
    "drizzle-orm": "^0.43.1",
    "fastify": "^5.3.0",
    "ioredis": "^5.6.1",
    "pg": "^8.15.6",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.19.4",
    "tsyringe": "^4.10.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/node": "^22.13.17",
    "@types/pg": "^8.11.14",
    "@vitest/coverage-v8": "^3.1.4",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "globals": "^16.0.0",
    "lefthook": "^1.11.13",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.31.0",
    "vitest": "^3.1.4"
  }
}
